{
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "pm2:start": "pm2 start 'npm run dev' --name nextjs-dev",
    "pm2:stop": "pm2 stop nextjs-dev",
    "pm2:restart": "pm2 restart nextjs-dev",
    "pm2:logs": "pm2 logs nextjs-dev --lines 100",
    "pm2:status": "pm2 status",
    "pm2:delete": "pm2 delete nextjs-dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:permissions": "vitest run src/lib/permissions src/hooks/__tests__/use* src/components/auth",
    "test:unit": "vitest run --dir src/lib --dir src/hooks --dir src/components",
    "test:integration": "vitest run --dir src/app/api/__tests__",
    "test:components": "vitest run --dir src/components",
    "test:watch": "vitest --watch",
    "test:debug": "vitest --inspect-brk --pool threads --poolOptions.threads.singleThread",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:permissions": "playwright test e2e/permissions",
    "seed:test-users": "tsx scripts/seed-test-users.ts",
    "import:component": "tsx scripts/import-components.ts import",
    "import:list": "tsx scripts/import-components.ts list",
    "import:all-shadcn": "tsx scripts/import-components.ts import --source shadcn --all"
  },
  "dependencies": {
